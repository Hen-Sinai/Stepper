<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="executedFlowData" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="component.executionResult.flowResult.ExecutedFlowDataController">
  <children>
    <TableView fx:id="flowDataTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
      <columns>
        <TableColumn fx:id="parameterColumn" editable="false" maxWidth="-1.0" minWidth="100.0" prefWidth="100.0" text="Parameter" />
        <TableColumn fx:id="dataColumn" maxWidth="1.7976931348623157E308" minWidth="200.0" prefWidth="400.0" text="Data" />
      </columns>
      <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
    </TableView>
  </children>
</VBox>
